! scrollbar style - none
URxvt.scrollBar: false
URxvt.depth: 32

! XFT
Xft.antialias:	true
! Font
URxvt*font:                           xft:Anonymous Pro:size=10
URxvt*boldFont:                       xft:Anonymous Pro:size=10
! URxvt*italicFont:                     Anonymous Pro
! URxvt*boldItalicFont:                 Anonymous Pro

urxvt.foreground: #d3d0c8
urxvt.background: #2d2d2d
URxvt.cursorColor: d3d0c8

! black
urxvt.color0: #2d2d2d
urxvt.color8: #848484

! red
urxvt.color1: #f2777a
urxvt.color9: #f2777a

! green
urxvt.color2: #99cc99
urxvt.color10: #99cc99

! yellow
urxvt.color3: #ffcc66
urxvt.color11: #ffcc66

! blue
urxvt.color4: #6699cc
urxvt.color12: #6699cc

! magenta
urxvt.color5: #cc99cc
urxvt.color13: #cc99cc

! cyan
urxvt.color6: #66cccc
urxvt.color14: #66cccc

! white
urxvt.color7: #d3d0c8
urxvt.color15: #f2f0ec

! Transparency
URxvt*.transparent: true
! URxvt* shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*.shading: 101
